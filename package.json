{
  "name": "netlist",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "bcrypt": "^2.0.1",
    "body-parser": "^1.18.3",
    "classnames": "^2.2.6",
    "custom-react-scripts": "^0.2.2",
    "express": "^4.16.3",
    "mongoose": "^5.1.5",
    "react": "^16.4.0",
    "react-dom": "^16.4.0",
    "react-router-dom": "^4.3.1"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "nodemon": "^1.17.5"
  },
  "scripts": {
    "start": "concurrently -kr \"npm run client\" \"npm run server\"",
    "client": "react-scripts start",
    "server": "nodemon server/index.js",
    "build": "react-scripts build"
  },
  "nodemonConfig": {
    "watch": [
      "server/*"
    ],
    "verbose": false
  },
  "homepage": "/netlist",
  "proxy": {
    "/netlist/api": {
      "target": "http://localhost:8080/"
    }
  }
}
